<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cloudbean.trackme"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_CONFIGURATION" />
    <uses-permission android:name="android.permission.WRITE_SETTINGS" />

    <meta-data
        android:name="com.amap.api.v2.apikey"
        android:value="8a5c4e51795aa5e9495b8006e91e77b9" />

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="22" />

    <application
        android:name=".TrackApp"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" android:hardwareAccelerated="true">
        <activity
            android:name="com.cloudbean.trackme.activity.MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.cloudbean.trackme.activity.CarGroupListActivity"
            android:label="@string/title_activity_car_list" >
        </activity>
        <activity
            android:name="com.cloudbean.trackme.activity.TraceActivity"
            android:label="@string/title_activity_map" >
        </activity>
        <activity
            android:name="com.cloudbean.trackme.activity.TimeChooseActivity"
            android:label="@string/title_activity_map" >
        </activity>
        <activity
            android:name="com.cloudbean.trackme.activity.ReplayActivity"
            android:label="@string/title_activity_reply" >
        </activity>
        <activity
            android:name="com.cloudbean.trackme.activity.MenuActivity"
            android:label="@string/title_activity_menu" >
        </activity>
        <activity
            android:name="com.cloudbean.trackme.activity.AlarmListActivity"
            android:label="@string/title_activity_alarm_list" >
        </activity>
        <activity
            android:name="com.cloudbean.trackme.activity.SetCommandActivity"
            android:label="@string/title_activity_set_command" >
        </activity>
        <activity
            android:name="com.cloudbean.trackme.activity.SplashActivity"
            android:label="@string/title_activity_splash" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.cloudbean.trackme.activity.BaseActivity"
            android:label="@string/title_activity_base" >
        </activity>
        <service android:name="com.cloudbean.trackme.server.NetWorkService"></service>
    </application>

</manifest>
